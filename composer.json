{
  "name": "runcmf/runtracy",
  "description": "Slim Framework Debugger",
  "license": "Apache-2.0",
  "type": "library",
  "keywords": [
    "Slim Framework",
    "runtracy",
    "Tracy",
    "Debug bar",
    "Debugbar",
    "Debugger",
    "Eloquent ORM",
    "PTY Terminal",
    "Profiler",
    "Slim3", "Slim 3"
  ],
  "authors": [
    {
      "name": "Vladimir",
      "email": "1f7.wizard@gmail.com",
      "homepage": "http://runcmf.ru"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "slim/slim": "~3.7",
    "tracy/tracy": "~2.4"
  },
  "require-dev": {
    "satooshi/php-coveralls": "1.*",
    "squizlabs/php_codesniffer": "^2.5",
    "phpunit/phpunit": "^5.7",
    "illuminate/database": "^5.3",
    "twig/twig": "~1.24",
    "slim/twig-view": "~2.1"
  },
  "autoload": {
    "psr-4": {
      "RunTracy\\": "src/RunTracy"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "@phpunit"
    ],
    "phpunit": "php vendor/bin/phpunit"
  }
}
